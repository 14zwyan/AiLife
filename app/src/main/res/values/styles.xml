<resources>
    <!--</style>-->
    <style name="AnimBottom">
        <item name="android:windowEnterAnimation">@anim/push_to_up</item>
    </style>
    <style name="AnimTop">
        <item name="android:windowEnterAnimation">@anim/slide_in_from_top</item>
    </style>
    <style name="AppBaseTheme">
    </style>
    <style name="AppTheme">
    </style>
    <style name="CardView">
        <item name="cardBackgroundColor">@color/cardview_light_background</item>
        <item name="cardCornerRadius">@dimen/cardview_default_radius</item>
        <item name="cardElevation">@dimen/cardview_default_elevation</item>
        <item name="cardMaxElevation">@dimen/cardview_default_elevation</item>
        <item name="cardUseCompatPadding">false</item>
        <item name="cardPreventCornerOverlap">true</item>
    </style>
    <style name="DialogWindowTitle">
        <item name="android:textAppearance">@android:style/TextAppearance.DialogWindowTitle</item>
        <item name="android:maxLines">1</item>
        <item name="android:scrollHorizontally">true</item>
    </style>
    <style name="GenericProgressBackground">
        <item name="android:gravity">left|center_horizontal|clip_horizontal</item>
        <item name="android:background">#dd111111</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
    </style>
    <style name="GenericProgressIndicator">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:indeterminate">true</item>
    </style>
    <style name="LoadingDialogStyle">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowFrame">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowAnimationStyle">@android:style/Animation.Dialog</item>
        <item name="android:backgroundDimEnabled">false</item>
    </style>
    <style name="MyDialogStyleBottom">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowFrame">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:backgroundDimEnabled">true</item>
    </style>
    <style name="MyDialogStyleBottom_v2">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowFrame">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowAnimationStyle">@style/AnimBottom</item>
        <item name="android:backgroundDimEnabled">true</item>
    </style>
    <style name="MyDialogStyleBottom_v3">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowAnimationStyle">@style/AnimBottom</item>
        <item name="android:backgroundDimEnabled">true</item>
    </style>
    <style name="MyDialogStyleUp">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowAnimationStyle">@style/AnimTop</item>
        <item name="android:backgroundDimEnabled">true</item>
    </style>
    <style name="MyTheme">
        <item name="android:alertDialogStyle">@style/dialog</item>
    </style>
    <style name="My_Theme_Dialog_Alert">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowTitleStyle">@style/DialogWindowTitle</item>
        <item name="android:backgroundDimEnabled">true</item>
    </style>

    <style name="Theme_Dialog_notitle">
        <item name="android:windowNoTitle">true</item>
    </style>
    <style name="Themegray">
        <item name="android:background">@color/gray</item>
    </style>
    <style name="Themegray_blace">
        <item name="android:background">@color/black</item>
    </style>
    <style name="animDialogPushDown">
        <item name="android:windowEnterAnimation">@anim/push_down_in</item>
        <item name="android:windowExitAnimation">@anim/push_up_out</item>
    </style>
    <style name="animDialogPushUp">
        <item name="android:windowEnterAnimation">@anim/push_up_in</item>
        <item name="android:windowExitAnimation">@anim/push_down_out</item>
    </style>
    <style name="base">
        <item name="android:padding">10dp</item>
    </style>
    <style name="body">
        <!--<item name="android:textStyle">@null</item>-->
        <item name="android:textColor">#000000</item>
    </style>
    <style name="customDialog">
        <item name="android:windowFrame">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowContentOverlay">@null</item>
    </style>
    <style name="customDialog1">
        <item name="android:windowFrame">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowContentOverlay">@null</item>
    </style>
    <style name="customDialogStyle">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowFrame">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowAnimationStyle">@android:style/Animation.Dialog</item>
        <item name="android:backgroundDimEnabled">false</item>
    </style>
    <style name="detail_description">
        <item name="android:textSize">13dp</item>
        <item name="android:textColor">#333333</item>
        <item name="android:ellipsize">end</item>
        <item name="android:paddingLeft">15dp</item>
        <item name="android:paddingTop">12dp</item>
        <item name="android:paddingRight">15dp</item>
        <item name="android:lineSpacingExtra">3dp</item>
    </style>
    <style name="dialog">
        <item name="android:fullDark">@drawable/dialog_body</item>
        <item name="android:topDark">@drawable/dialog_title</item>
        <item name="android:centerDark">@drawable/dialog_body</item>
        <item name="android:bottomDark">@drawable/dialog_footer</item>
        <item name="android:fullBright">@drawable/dialog_body</item>
        <item name="android:centerBright">@drawable/dialog_body</item>
        <item name="android:bottomMedium">@drawable/dialog_footer</item>
        <item name="android:centerMedium">@drawable/dialog_body</item>
    </style>
    <style name="mycheckbox">
        <item name="android:button">@drawable/mycheckbox</item>
    </style>
    <style name="popup_anim">
        <item name="android:windowEnterAnimation">@anim/popup_up</item>
        <item name="android:windowExitAnimation">@anim/popup_down</item>
    </style>
    <style name="rating_bar_style">
        <item name="android:maxHeight">40dp</item>
        <item name="android:progressDrawable">@drawable/ratingstars</item>
        <item name="android:minHeight">40dp</item>
    </style>
    <style name="style_padding_16">
        <item name="android:paddingLeft">@dimen/margin_activity_vertical</item>
        <item name="android:paddingTop">@dimen/margin_activity_vertical</item>
        <item name="android:paddingRight">@dimen/margin_activity_vertical</item>
        <item name="android:paddingBottom">@dimen/margin_activity_vertical</item>
    </style>
    <style name="style_padding_left_rigt_16">
        <item name="android:paddingLeft">@dimen/margin_activity_vertical</item>
        <item name="android:paddingRight">@dimen/margin_activity_vertical</item>
    </style>
    <style name="style_txt_add_digit">
        <item name="android:textSize">@dimen/textSize_Medium</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textColorHint">@color/black_tran_ts</item>
    </style>
    <style name="style_txt_content">
        <item name="android:textSize">@dimen/textSize_Small_14</item>
        <item name="android:textColor">@color/black_tran_fs</item>
        <item name="android:textColorHint">@color/black_tran_ts</item>
    </style>
    <style name="style_txt_prompt">
        <item name="android:textSize">@dimen/textSize_TW_12</item>
        <item name="android:textColor">@drawable/sel_txt_prompt</item>
        <item name="android:textColorHint">@color/black_tran_ts</item>
    </style>
    <style name="style_txt_title">
        <item name="android:textSize">@dimen/textSize_Small_14</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textColorHint">@color/black_tran_ts</item>
    </style>
    <style name="text_message_count">
        <item name="android:textSize">13dp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">#ffffff</item>
        <item name="android:gravity">left|center_horizontal|clip_horizontal</item>
        <item name="android:background">#f24949</item>
        <item name="android:layout_width">20dp</item>
        <item name="android:layout_height">20dp</item>
    </style>
    <style name="text_message_count_tab">
        <item name="android:textSize">10dp</item>
        <item name="android:paddingLeft">2dp</item>
        <item name="android:paddingRight">2dp</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">14dp</item>
        <item name="android:minWidth">14dp</item>
        <item name="android:includeFontPadding">false</item>
    </style>
    <style name="title">
        <item name="android:textSize">16sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">#ffffff</item>
    </style>
</resources>